DROP TABLE IF EXISTS TYPE;

CREATE TABLE TYPE (
	TYPE_ID INT PRIMARY KEY,
	GROUP_ID INT,
	TYPE_NAME VARCHAR,
	DESCRIPTION VARCHAR,
	MASS DOUBLE,
	VOLUME DOUBLE,
	CAPACITY DOUBLE,
	PORTION_SIZE INT,
	RACE_ID INT,
	BASE_PRICE DOUBLE,
	PUBLISHED BOOLEAN,
	MARKET_GROUP_ID INT,
	CHANCE_OF_DUPLICATING DOUBLE,
	ICON_ID INT
);

INSERT INTO 
	TYPE
SELECT
	*
FROM
	CSVREAD('classpath:\META-INF\static_data\invTypes.csv');
