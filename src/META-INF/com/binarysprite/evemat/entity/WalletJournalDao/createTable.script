DROP TABLE IF EXISTS WALLET_JOURNAL;

CREATE TABLE WALLET_JOURNAL (
	REF_ID BIGINT PRIMARY KEY,
	DATE TIMESTAMP,
	REF_TYPE_ID INT,
	OWNER_NAME_1 VARCHAR,
	OWNER_ID_1 INT,
	OWNER_NAME_2 VARCHAR,
	OWNER_ID_2 INT,
	ARG_NAME_1 VARCHAR,
	ARG_ID_1 INT,
	AMOUNT DOUBLE,
	BALANCE DOUBLE,
	REASON VARCHAR,
	TAX_RECEIVER VARCHAR,
	TAX_AMOUNT VARCHAR,
	ACCOUNT_KEY INT,
	CHARACTER_ID BIGINT
);
