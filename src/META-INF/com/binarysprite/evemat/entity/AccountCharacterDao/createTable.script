DROP TABLE IF EXISTS ACCOUNT_CHARACTER;

CREATE CACHED TABLE PUBLIC.ACCOUNT_CHARACTER(
    CHARACTER_ID BIGINT NOT NULL,
    CHARACTER_NAME VARCHAR,
    CORPORATION_ID BIGINT,
    COPORATION_NAME VARCHAR,
    API_ID INT,
    API_VERIFICATION_CODE VARCHAR,
    SKILL_INDUSTRY INT DEFAULT 0,
    SKILL_PRODUCTION_EFFICIENCY INT DEFAULT 0
);
